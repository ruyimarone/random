{
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "editor.fontSize": 12,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<leader>", "/"],
            "commands": [":set hls!"]
        },
        {
            "before": ["<leader>", "l"],
            "commands": [":set nu!"]
        },
        {
            "before": ["<leader>", "c", "i"],
            "commands": ["editor.action.commentLine"]
        },
        {
            "before": ["<leader>", "c", "c"],
            "commands": ["editor.action.addCommentLine"]
        },
        {
            "before": ["<leader>", "c", "u"],
            "commands": ["editor.action.removeCommentLine"]
        },
        {
            "before": ["<leader>", "n"],
            "commands": ["workbench.action.nextEditorInGroup"]
        },
        {
            "before": ["<leader>", "p"],
            "commands": ["workbench.action.previousEditorInGroup"]
        },
        {
            "before": ["<leader>", "w"],
            "commands": ["editor.action.toggleWordWrap"]
        },
        {
            "before": ["<leader>", "i"],
            "commands": ["editor.action.showHover"]
        },
        {
            "before": ["u"],
            "commands": ["undo"]
        },
        {
            "before": ["<C-r>"],
            "commands": ["redo"]
        },
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["<leader>", "c", "i"],
            "commands": ["editor.action.commentLine"]
        },
        {
            "before": ["<leader>", "c", "c"],
            "commands": ["editor.action.addCommentLine"]
        },
        {
            "before": ["<leader>", "c", "u"],
            "commands": ["editor.action.removeCommentLine"]
        },
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    // "editor.quickSuggestions": {
    //     "other": true,
    //     "comments": false,
    //     "strings": false
    //   },
    // "editor.quickSuggestionsDelay": 10,
    "editor.lineNumbers": "on",
    "workbench.colorCustomizations": {
        "[One Dark Pro]": {
            "editor.background": "#191c1f"
        },
    },
    "workbench.fontAliasing": "auto",
    "remote.autoForwardPorts": false,
    "terminal.integrated.macOptionClickForcesSelection": true,
    "editor.minimap.enabled": false,
    "terminal.integrated.inheritEnv": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.wordWrap": "on",
    "terminal.integrated.profiles.osx": {
        "bash-brew": {
            "args": [
                "-l"
            ],
            "path": "/opt/homebrew/bin/bash"
        }
    },
    "terminal.integrated.defaultProfile.osx": "bash-brew",
    "editor.inlineSuggest.enabled": true,
    "oneDarkPro.vivid": false,
    "remote.SSH.connectTimeout": 30,
    "telemetry.telemetryLevel": "off",
    "python.analysis.autoImportCompletions": false,
    "terminal.integrated.scrollback": 10000,
    "editor.tabCompletion": "on",
    "editor.wordBasedSuggestions": "matchingDocuments",
    "jupyter.interactiveWindow.creationMode": "perFile",
    "notebook.cellFocusIndicator": "border",
    "python.languageServer": "None",
    "python.analysis.diagnosticSeverityOverrides": {
        "reportUnknownVariableType": "none",
        "reportUnknownMemberType" : "none"
    },
    "jupyter.askForKernelRestart": false,
    "workbench.colorTheme": "One Dark Pro",
    "editor.semanticHighlighting.enabled": false,
    "editor.suggest.localityBonus": true,
    "editor.suggest.preview": true,
    "notebook.output.wordWrap": true,
    "editor.quickSuggestionsDelay": 1,
    "editor.wordBasedSuggestionsMode": "currentDocument",
    "workbench.list.smoothScrolling": true,
    "window.commandCenter": false,
    "[python]": {
        "editor.wordBasedSuggestions": "matchingDocuments"
    },
    "interactiveWindow.executeWithShiftEnter": true,
    "editor.acceptSuggestionOnEnter": "off",
    "python.terminal.activateEnvironment": false,
    "terminal.integrated.altClickMovesCursor": false,
    "notebook.output.scrolling": true,
    "editor.smoothScrolling": true,
    "jupyter.runStartupCommands": [
        "%load_ext autoreload",
        "%autoreload 2"
    ],
    "window.dialogStyle": "custom",
    "pylint.args": [
        "--disable=all",
        "--enable=F,E,unreachable,duplicate-key,unnecessary-semicolon,global-variable-not-assigned,unused-variable,binary-op-exception,bad-format-string,anomalous-backslash-in-string,bad-open-mode"
    ],
}
